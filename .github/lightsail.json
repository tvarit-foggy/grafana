{
  "serviceName": "dev-grafana-<PR_NUMBER/>",
  "containers": {
     "dev-grafana-<PR_NUMBER/>": {
        "image": "<IMAGE/>",
        "environment": {
          "GF_SMTP_ENABLED": "true",
          "GF_SMTP_HOST": "<SMTP_HOST/>",
          "GF_SMTP_USER": "<SMTP_USER/>",
          "GF_SMTP_PASSWORD": "<SMTP_PASSWORD/>",
          "GF_SMTP_FROM_ADDRESS": "<SMTP_USER/>",
          "GF_SMTP_FROM_NAME": "[dev-<PR_NUMBER/>] Tvarit AI Platform",
          "GF_FEATURE_TOGGLES_ENABLE": "newNavigation",
          "GF_PLUGINS_ENABLE_ALPHA": "true"
        },
        "ports": {
           "3000": "HTTP"
        }
     }
  },
  "publicEndpoint": {
     "containerName": "dev-grafana-<PR_NUMBER/>",
     "containerPort": 3000
  }
}
